{
  "routes": [
    {
      "route": "/*.js",
      "mimeType": "application/javascript"
    },
    {
      "route": "/*.mjs",
      "mimeType": "application/javascript"
    },
    {
      "route": "/*.jsx",
      "mimeType": "application/javascript"
    },
    {
      "route": "/*.tsx",
      "mimeType": "application/javascript"
    },
    {
      "route": "/*.ts",
      "mimeType": "text/typescript"
    },
    {
      "route": "/*.css",
      "mimeType": "text/css"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/.well-known/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
